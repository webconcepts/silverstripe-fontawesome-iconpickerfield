{
    "name": "thisisbd/silverstripe-fontawesome-iconpickerfield",
    "description": "Font Awesome Icon Picker for SilverStripe 3.1.x based on http://mjolnic.com/fontawesome-iconpicker",
    "homepage": "http://github.com/thisisbd/silverstripe-fontawesome-iconpickerfield",
    "keywords": [
        "silverstripe","font awesome","icon picker"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {"name": "Darren-Lee Joseph","email": "darrenleejoseph@gmail.com"}
    ],
    "support": {
        "issues": "http://github.com/thisisbd/silverstripe-fontawesome-iconpickerfield/issues"
    },
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4"
    },
    "autoload": {
        "psr-4": {
            "Thisisbd\\FontAwesomeIconPickerField\\": "code/"
        }
    },
    "extra": {
        "installer-name": "fontawesome-iconpickerfield",
        "expose": [
            "assets"
        ]
    }
}
